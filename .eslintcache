r validacep = /^[0-9]{8}$/;\n\n    if (validacep.test(cep)) {\n      document.getElementById('logradouro').value = \"...\";\n      document.getElementById('bairro').value = \"...\";\n      document.getElementById('cidade').value = \"...\";\n      document.getElementById('estado').value = \"...\";\n\n      getAddress(cep);\n\n    } else {\n      alert('Formato de CEP inválido.');\n      limpa_form_cep();\n    }\n  } else {\n    limpa_form_cep();\n  }\n}\n\nfunction submitOk() {\n  const inputs = document.querySelectorAll('[required]');\n\n  console.log(inputs);\n  \n  for(let input of inputs) {\n    input.addEventListener(\"invalid\", event => {\n      console.log(\"Campos invalidos\");\n    })\n  }\n}\n\nconst form = document.querySelector('form');\nif(form) {\n  form.addEventListener(\"submit\", event => {\n    alert(\"Formulário enviado.\");\n  });\n}\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <form action=\".\" method=\"get\">\n        <fieldset>\n          <legend>Formulário</legend>\n          <div className=\"pag1\">\n            <label>\n              Nome* <br />\n              <input type=\"text\" id=\"nome\" required autoFocus />\n            </label>\n            <label>\n              E-mail* <br />\n              <input type=\"email\" id=\"email\" required />\n            </label>\n            <label>\n              Telefone* <br />\n              <MaskedInput\n                mask={['(', /[1-9]/, /\\d/, ')', ' ', /\\d/, /\\d/, /\\d/, /\\d/, /\\d/, '-', /\\d/, /\\d/, /\\d/, /\\d/]}\n                type=\"tel\" id=\"telefone\"\n                required\n              />\n            </label>\n            <label>\n              CEP* <br />\n              <MaskedInput\n                mask={[/[1-9]/, /\\d/, /\\d/, /\\d/, /\\d/, '-', /\\d/, /\\d/, /\\d/]}\n                type=\"tel\" id=\"cep\" minLength=\"8\"\n                onBlur={eventCep} required\n              />\n            </label>\n            <label>\n              Logradouro* <br />\n              <input type=\"text\" id=\"logradouro\" required />\n            </label>\n            <p> * Campos obrigatórios.</p>\n          </div>\n          <div className=\"pag2\">\n            <label>\n              Número* <br />\n              <input type=\"text\" id=\"numero\" required />\n            </label>\n            <label>\n              Complemento <br />\n              <input type=\"text\" id=\"complemento\" />\n            </label>\n            <label>\n              Bairro* <br />\n              <input type=\"text\" id=\"bairro\" required />\n            </label>\n            <label>\n              Cidade* <br />\n              <input type=\"text\" id=\"cidade\" required />\n            </label>\n            <label>\n              Estado* <br />\n              <input type=\"text\" id=\"estado\" required />\n            </label>\n            <button type=\"submit\" onClick={submitOk}>Enviar</button>\n          </div>\n        </fieldset>\n      </form>\n    </div>\n  );\n}\n\nexport default App;\n","/home/heifel/Documentos/Web_Dev/form-test/src/index.js",[],"/home/heifel/Documentos/Web_Dev/teste-bemobile/src/App.js",[],{"ruleId":"17","severity":1,"message":"18","line":1,"column":17,"nodeType":"19","messageId":"20","endLine":1,"endColumn":30},"no-unused-vars","'createElement' is defined but never used.","Identifier","unusedVar"]